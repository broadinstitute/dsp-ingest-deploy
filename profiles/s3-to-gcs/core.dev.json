{
  "name": "ingest",
  "intent": "S3->GCS testing in dev",
  "production": false,
  "stability": "volatile",
  "environments": {
      "default": {
          "name": "dev"
      }
  },
  "projects": {
      "default": {
          "name": "broad-dsp-monster-dev",
          "provider_sa_path": "secret/devops/terraform/dev/monster/terraform-service-account.json"
      },
      "state_bucket": {
          "provider_sa_path": "secret/dsp/service-accts/dsp-techops/terraform-state-service-account.json"
      }
  },
  "terraform_state": {
      "default": {
          "state_bucket": "broad-dsp-terraform-state",
          "state_path_prefix": "monster"
      }
  },
  "dns": {
      "default": {
          "name": "monster-dev",
          "zone": "monster-dev.broadinstitute.org"
      }
  },
  "regions": {
      "default": {
          "name": "us-central-1",
          "zone_name": "a"
      }
  },
  "clusters": {
      "default": {
          "name": "ingest-k8s-cluster"
      }
  },
  "profile_vars": {
      "s3-to-gcs": {
          "env": {
              "INGEST_PROJECT": "core",
              "CORE_PROJECT": "broad-dsp-monster-dev",
              "K8S_NODE_COUNT": 3,
              "K8S_MACHINE_TYPE": "n1-standard-2",
              "ENABLE_DNS": "true"
          }
      }
  },
  "terraform_image": "gcr.io/broad-dsp-gcr-public/terraform0.12:0.12.5"
}
